{
  "name": "dravencms/carousel",
  "type": "dravencms-package",
  "description": "Carousel module for dravencms",
  "keywords": [
    "dravencms",
    "carousel"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/file": "^v1.0",
    "dravencms/admin": "^v1.0",
    "dravencms/structure": "^v1.0"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "carousel": "Dravencms\\Carousel\\DI\\CarouselExtension"
        }
      },
      "scripts": {
        "post-install": "Dravencms\\Carousel\\Script\\PostInstall",
        "pre-uninstall": "Dravencms\\Carousel\\Script\\PreUninstall"
      }
    }
  }
}
